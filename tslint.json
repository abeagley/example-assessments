{
  "extends": ["tslint-react", "tslint-config-standard"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "no-unnecessary-type-assertion": [false],
    "jsx-no-lambda": [false]
  }
}
